<%= form_for @category do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :daily_rate %>
    <%= f.number_field :daily_rate, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :car_insurance %>
    <%= f.number_field :car_insurance, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :third_party_insurance %>
    <%= f.number_field :third_party_insurance, class: 'form-control' %>
  </div>
  <%= f.submit 'Enviar', class: 'btn btn-primary' %>
<% end %>